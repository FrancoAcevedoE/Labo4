@model IEnumerable<PROGRAM.Models.OrdenDeTrabajo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TipoOT)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ManoDeObra)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Equipo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Procedimiento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Material)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TipoOT)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ManoDeObra.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Equipo.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Procedimiento.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Material.ID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
